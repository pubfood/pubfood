{
  "name": "pubfood",
  "version": "1.1.0",
  "description": "A free and open source header bidding framework built by publishers for publishers",
  "keywords": [
    "header",
    "bidding",
    "auction",
    "advertising",
    "publisher",
    "prebid"
  ],
  "homepage": "http://pubfood.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/pubfood/pubfood.git"
  },
  "bugs": {
    "url": "https://github.com/pubfood/pubfood/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Pubfood",
    "url": "https://github.com/pubfood/pubfood/graphs/contributors"
  },
  "contributors": [],
  "main": "./dist/pubfood.js",
  "scripts": {
    "clean": "npm prune --production && rm -fr build",
    "prepare": "mkdir -p build",
    "lint": "gulp lint",
    "test": "gulp test",
    "build": "npm install && npm run prepare && npm run lint && gulp build && npm run test",
    "start": "npm run build",
    "release": "PACKAGE_VER=`node -e 'console.log(require(\"./package.json\").version)'` && git commit -m \"v${PACKAGE_VER}\" && git tag -a v${PACKAGE_VER} -m \"v${PACKAGE_VER}\" && git push && git push --tags"
  },
  "browserify": {
    "standalone": "pubfood",
    "entries": [
      "./src/pubfood.js"
    ]
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "chai": "^3.3.0",
    "del": "^2.0.2",
    "eventemitter3": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-exit": "0.0.2",
    "gulp-header": "^1.7.1",
    "gulp-mocha": "^2.1.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^1.4.1",
    "sinon": "^1.17.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
